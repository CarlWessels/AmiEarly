<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
      <bindings>
        <wsHttpBinding>
          <binding name="wsSecureService" sendTimeout="00:05:00">
            <security mode="TransportWithMessageCredential">
              <transport clientCredentialType="None" />
              <message clientCredentialType="UserName" />
            </security>
          </binding>
        </wsHttpBinding>
      </bindings>
      <client>
        <endpoint address="https://localhost:24599/MerchantService.svc"
            binding="wsHttpBinding" bindingConfiguration="wsSecureService"
            contract="MerchantService.IMerchantService" name="wsSecureService" />
      </client>
    </system.serviceModel>
</configuration>